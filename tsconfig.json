{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ES2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    // "vendor",
    "node_modules"
  ],
  // "$schema": "https://json.schemastore.org/tsconfig",
  // "display": "Recommended",
}